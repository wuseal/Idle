project 'iosApp.xcodeproj'

# Uncomment the next line to define a global platform for your project
platform :ios, '12.0'

target 'iosApp' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for iosApp
  # depend on kotlin soure code
  pod 'mpplib', :path => '../mpplib'
  # if need to depend on released pod, uncomment next line and comment upon line
  # with this method, iOS build will not triggle gradle tasks
  # pod 'mpplib', :path => '../Idle.podspec'
  pod 'Toaster',"~> 2.3.0"
  pod 'Nuke', "9.2.2"

end
target 'iosAppTests' do
  inherit! :search_paths
  # Pods for testing
end
